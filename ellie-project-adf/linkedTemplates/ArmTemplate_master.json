{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ellie-project-adf"
		},
		"AzureBlobStorageLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageLinkedService'"
		},
		"AzureSqlDWLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDWLinkedService'"
		},
		"AzureSqlDatabaseLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseLinkedService'"
		},
		"AzureStorageLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureStorageLinkedService'"
		},
		"Azuresqldatabasels1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Azuresqldatabasels1'"
		},
		"Elliebloblinked_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Elliebloblinked'"
		},
		"LocalSQLServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LocalSQLServer'"
		},
		"lswatermark_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lswatermark'"
		},
		"AzureBlobStoragewatermark_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStoragewatermark'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ellie-project-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureStorageLinkedService_connectionString": {
						"value": "[parameters('AzureStorageLinkedService_connectionString')]"
					},
					"Azuresqldatabasels1_connectionString": {
						"value": "[parameters('Azuresqldatabasels1_connectionString')]"
					},
					"lswatermark_connectionString": {
						"value": "[parameters('lswatermark_connectionString')]"
					},
					"AzureBlobStoragewatermark_connectionString": {
						"value": "[parameters('AzureBlobStoragewatermark_connectionString')]"
					},
					"AzureBlobStorageLinkedService_connectionString": {
						"value": "[parameters('AzureBlobStorageLinkedService_connectionString')]"
					},
					"AzureSqlDWLinkedService_connectionString": {
						"value": "[parameters('AzureSqlDWLinkedService_connectionString')]"
					},
					"AzureSqlDatabaseLinkedService_connectionString": {
						"value": "[parameters('AzureSqlDatabaseLinkedService_connectionString')]"
					},
					"Elliebloblinked_connectionString": {
						"value": "[parameters('Elliebloblinked_connectionString')]"
					},
					"LocalSQLServer_connectionString": {
						"value": "[parameters('LocalSQLServer_connectionString')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ellie-project-adf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ellie-project-adf_ArmTemplate_0')]"
			]
		}
	]
}